frappe.ui.form.on("Delivery Schedule", {
update_price: function(frm) {
frappe.call({
method: "Delivery Schedule.Delivery Schedule.custom_py_methods.show_route",
args: {
"doc": frm.doc,
"show_route": frm.doc.show_route
},
callback: function(r) {
if(r.message=="success"){
console.log(r.message);

cur_frm.save();
frappe.msgprint("Item Price updated");
}
}
})
}
});